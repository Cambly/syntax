{
  "name": "root",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "pnpm run dev",
    "build": "turbo run build",
    "changeset": "changeset",
    "clean": "turbo run clean && rm -rf node_modules",
    "dev-package-json": "tsx scripts/build/package-json-dev.ts",
    "dev-turborepo": "turbo run dev --no-cache --continue",
    "dev": "conc -r 'pnpm:dev-package-json' 'pnpm:dev-turborepo'",
    "format": "prettier --write \"**/*.{ts,tsx,md,css}\"",
    "lint": "turbo run lint",
    "release": "turbo run build && changeset publish && pnpm install --no-frozen-lockfile",
    "stylelint": "turbo run stylelint",
    "test:watch": "turbo run test:watch",
    "test": "turbo run test",
    "version-packages": "changeset version && pnpm install --no-frozen-lockfile",
    "generate": "npx tsx './generate-component/' $1"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/cross-spawn": "6.0.6",
    "@types/fs-extra": "11.0.4",
    "@types/prettier": "2.7.2",
    "@vitejs/plugin-react": "4.0.1",
    "concurrently": "8.2.0",
    "eslint": "8.39.0",
    "glob": "10.3.4",
    "jsdom": "22.0.0",
    "prettier": "2.8.8",
    "rimraf": "^5.0.1",
    "stylelint": "15.6.0",
    "stylelint-config-css-modules": "4.2.0",
    "stylelint-config-standard": "33.0.0",
    "tsx": "4.6.2",
    "turbo": "1.9.0",
    "vite": "4.3.4",
    "vitest": "0.32.4"
  },
  "engines": {
    "npm": "please-use-pnpm",
    "pnpm": "^8.6.6"
  }
}
