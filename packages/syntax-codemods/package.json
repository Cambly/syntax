{
  "name": "@cambly/syntax-codemods",
  "version": "0.7.0",
  "description": "Cambly design system codemods",
  "bin": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "Apache-2.0",
  "files": [
    "dist/**"
  ],
