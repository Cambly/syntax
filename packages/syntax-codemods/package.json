{
  "name": "@cambly/syntax-codemods",
  "version": "0.2.0",
  "description": "Codemods for Cambly's design system",
  "bin": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "Apache-2.0",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup --watch",
    "lint": "TIMING=1 eslint \"src/**/*.ts*\"",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "jscodeshift": "0.14.0"
  },
  "devDependencies": {
    "@cambly/eslint-config-syntax": "workspace:*",
    "@cambly/syntax-tsconfig": "workspace:*",
    "@types/jscodeshift": "^0.11.0",
    "eslint": "^8.35.0",
    "tsup": "6.7.0",
    "typescript": "5.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
